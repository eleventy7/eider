<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sbe:messageSchema xmlns:sbe="http://fixprotocol.io/2016/sbe"
                   package="io.eider.protocol"
                   id="688"
                   version="1"
                   semanticVersion="0.1"
                   description="Eider Protocol Messages"
                   byteOrder="littleEndian">
    <types>
        <composite name="messageHeader" description="Message identifiers and length of message root">
            <type name="blockLength" primitiveType="uint16"/>
            <type name="templateId" primitiveType="uint16"/>
            <type name="schemaId" primitiveType="uint16"/>
            <type name="version" primitiveType="uint16"/>
        </composite>
        <composite name="Utf8String">
            <type name="length" primitiveType="uint16"/>
            <type name="varData" primitiveType="uint8" length="0" characterEncoding="UTF-8"/>
        </composite>
        <composite name="AsciiString">
            <type name="length" primitiveType="uint16"/>
            <type name="varData" primitiveType="char" length="0"/>
        </composite>
        <composite name="varStringEncoding">
            <type name="length" primitiveType="uint32" maxValue="1073741824"/>
            <type name="varData" primitiveType="uint8" length="0" characterEncoding="UTF-8"/>
        </composite>
    </types>
    <types>
        <type name="Sequence" primitiveType="int64"/>
        <type name="MessageType" primitiveType="int32"/>
        <type name="Timestamp" primitiveType="int64"/>
    </types>

    <sbe:message name="IpcHeader" id="1" description="Header for internal IPC communications">
        <field name="messageType" id="1" type="MessageType"/>
        <data name="sender" id="2" type="Utf8String"/>
    </sbe:message>

    <sbe:message name="UdpHeader" id="2" description="Header for internal UDP communications">
        <field name="messageType" id="1" type="MessageType"/>
        <field name="senderStream" id="2" type="int32"/>
        <field name="sequence" id="3" type="Sequence"/>
        <data name="sender" id="4" type="Utf8String"/>
        <data name="senderChannel" id="5" type="Utf8String"/>
    </sbe:message>

    <sbe:message name="ClientRegistration" id="3" description="Client Registration">
        <field name="responseStream" id="1" type="int32"/>
        <data name="responseChannel" id="2" type="Utf8String"/>
    </sbe:message>

    <sbe:message name="ClientRegistrationResponse" id="4" description="Client Registration">
        <field name="sessionId" id="1" type="int32"/>
    </sbe:message>

</sbe:messageSchema>